check host fbrell with address 127.0.0.1
  start program "/home/naitik/.nave/installed/latest/bin/node /data/www/fbrell/server.js"
    as uid naitik and gid naitik
  stop program "/home/naitik/.nave/installed/latest/bin/node /data/www/fbrell/server.js --stop"
    as uid naitik and gid naitik
  if failed port 43600 protocol HTTP
    request /
    with timeout 5 seconds
    then restart
  if 5 restarts within 5 cycles then timeout
